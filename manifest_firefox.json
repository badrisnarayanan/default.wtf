{
  "name": "Default Account+ for Google",
  "description": "Select and manage your default account for Google Meet, Gmail, or any other Google product. A maintained fork of default.wtf.",
  "version": "2.0.0",
  "manifest_version": 2,
  "browser_specific_settings": {
    "gecko": {
      "id": "default-account-plus@badrisnarayanan",
      "strict_min_version": "57.0"
    }
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["utils.js", "background-firefox.js"],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "commands": {
    "switch_to_ga_1": {
      "suggested_key": {
        "default": "Alt+1",
        "mac": "Alt+1"
      },
      "description": "Switch to Google Account #1"
    },
    "switch_to_ga_2": {
      "suggested_key": {
        "default": "Alt+2",
        "mac": "Alt+2"
      },
      "description": "Switch to Google Account #2"
    },
    "switch_to_ga_3": {
      "suggested_key": {
        "default": "Alt+3",
        "mac": "Alt+3"
      },
      "description": "Switch to Google Account #3"
    },
    "switch_to_ga_4": {
      "suggested_key": {
        "default": "Alt+4",
        "mac": "Alt+4"
      },
      "description": "Switch to Google Account #4"
    },
    "switch_to_ga_5": {
      "description": "Switch to Google Account #5"
    },
    "switch_to_ga_6": {
      "description": "Switch to Google Account #6"
    },
    "switch_to_ga_7": {
      "description": "Switch to Google Account #7"
    },
    "switch_to_ga_8": {
      "description": "Switch to Google Account #8"
    },
    "switch_to_ga_9": {
      "description": "Switch to Google Account #9"
    }
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "128": "/images/128.png"
  }
}
